# 01_limpieza.ipynb

import pandas as pd
from src.limpieza import cargar_datos, limpiar_datos

# Cargar dataset de ejemplo
archivo = "data/raw/accidentes.csv"  # cambia al nombre de tu CSV
df = cargar_datos(archivo)

# Mostrar primeras filas
df.head()

# Limpiar datos
df_clean = limpiar_datos(df)

# Guardar datos limpios
df_clean.to_csv("data/cleaned/accidentes_limpios.csv", index=False)

# Comprobaci√≥n
